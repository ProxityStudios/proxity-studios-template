{
	"name": "proxity-studios-template",
	"version": "0.0.0",
	"repository": "https://github.com/ProxityStudios/proxity-studios-template.git",
	"author": "@proxitystudios",
	"license": "MIT",
	"private": false,
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write src/**/*.ts",
		"pq": "pretty-quick",
		"update": "yarn upgrade-interactive",
		"sversion": "standard-version",
		"build": "tsc -b src",
		"clean": "tsc -b src --clean",
		"watch": "tsc -b src -w"
	},
	"devDependencies": {
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^2.6.2",
		"pretty-quick": "^3.1.3",
		"standard-version": "^9.5.0"
	},
	"engines": {
		"node": ">=v14.0.0",
		"npm": ">=7.0.0"
	}
}
